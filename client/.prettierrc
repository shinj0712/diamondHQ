{
	"printWidth": 100,
	"trailingComma": "es5",
	"tabWidth": 2,
	"useTabs": false,
	"semi": true,
	"singleQuote": true,
	"quoteProps": "as-needed",
	"jsxSingleQuote": true,
	"bracketSpacing": true,
	"bracketSameLine": false,
	"jsxBracketSameLine": false,
	"endOfLine": "lf",
	"arrowParens": "always",
	"requirePragma": false,
	"insertPragma": false,
	"htmlWhitespaceSensitivity": "ignore",
	"singleAttributePerLine": false
}
